  <Requirements:Requirement xmi:id="_363T4MrXEee9jsGys1t6BQ" base_Class="_36368MrXEee9jsGys1t6BQ" id="SH_SEC_010" text="The Smart Home must prevent unauthorized physical intrusion."/>
  <Requirements:Requirement xmi:id="_363T4crXEee9jsGys1t6BQ" base_Class="_366-QMrXEee9jsGys1t6BQ" id="SH_SEC_020" text="The Smart Home must prevent computer intrusion."/>
  <Requirements:Requirement xmi:id="_363T4srXEee9jsGys1t6BQ" base_Class="_366-QcrXEee9jsGys1t6BQ" id="SH_SEC_030" text="The Smart Home must provide an identification-based access control system."/>
  <Requirements:Requirement xmi:id="_363T48rXEee9jsGys1t6BQ" base_Class="_367lUMrXEee9jsGys1t6BQ" id="SH_SEC_040" text="The Smart Home must call the police station in case of burglary or any other unauthorized intrusion."/>
  <Requirements:Requirement xmi:id="_363T5MrXEee9jsGys1t6BQ" base_Class="_367lUcrXEee9jsGys1t6BQ" id="SH_SEC_050" text="The Smart Home must be able to detect smoke and to call the fire station in case of suspicion of fire."/>
  <Requirements:Requirement xmi:id="_363T5crXEee9jsGys1t6BQ" base_Class="_367lUsrXEee9jsGys1t6BQ" id="SH_SEC_060" text="The Smart Home must be able to detect accidental falls and to call an ambulance."/>
  <Requirements:Requirement xmi:id="_363T5srXEee9jsGys1t6BQ" base_Class="_367lU8rXEee9jsGys1t6BQ" id="SH_SEC_070" text="The Smart Home must provide authorized access for emergency services (firemen, hospital service, etc.)."/>
  <Requirements:Requirement xmi:id="_363T58rXEee9jsGys1t6BQ" base_Class="_367lVMrXEee9jsGys1t6BQ" id="SH_ACO_010" text="The Smart Home must accommodate physical and mental conditions of the inhabitants."/>
  <Requirements:Requirement xmi:id="_363T6MrXEee9jsGys1t6BQ" base_Class="_367lVcrXEee9jsGys1t6BQ" id="SH_ACO_020" text="The Smart Home must accommodate specific preferences entered by the inhabitants."/>
  <Requirements:Requirement xmi:id="_363T6crXEee9jsGys1t6BQ" base_Class="_367lVsrXEee9jsGys1t6BQ" id="SH_ACO_030" text="The Smart Home must learn from the behaviour of inhabitants."/>
  <Requirements:Requirement xmi:id="_363T6srXEee9jsGys1t6BQ" base_Class="_367lV8rXEee9jsGys1t6BQ" id="SH_ACO_040" text="The Smart Home must assist the inhabitants with the everyday tasks regardless of their disabilities."/>
  <Requirements:Requirement xmi:id="_363T68rXEee9jsGys1t6BQ" base_Class="_367lWMrXEee9jsGys1t6BQ" id="SH_ACO_050" text="The Smart Home must be monitorable via a central interface located in the main room."/>
  <Requirements:Requirement xmi:id="_363T7MrXEee9jsGys1t6BQ" base_Class="_367lWcrXEee9jsGys1t6BQ" id="SH_ACO_060" text="The Smart Home must support the connection of smart devices of the inhabitants."/>
  <Requirements:Requirement xmi:id="_363T7crXEee9jsGys1t6BQ" base_Class="_367lWsrXEee9jsGys1t6BQ" id="SH_ACO_070" text="The Smart Home must have a bed that accommodates to the disability of the inhabitants."/>
  <Requirements:Requirement xmi:id="_363T7srXEee9jsGys1t6BQ" base_Class="_367lW8rXEee9jsGys1t6BQ" id="SH_ECO_010" text="The Smart Home must save energy."/>
  <Requirements:Requirement xmi:id="_363T78rXEee9jsGys1t6BQ" base_Class="_367lXMrXEee9jsGys1t6BQ" id="SH_ECO_020" text="The Smart Home annual consumption should be less than local regulations."/>
  <Requirements:Requirement xmi:id="_363T8MrXEee9jsGys1t6BQ" base_Class="_367lXcrXEee9jsGys1t6BQ" id="SH_ECO_030" text="The Smart Home must prevent unauthorized physical intrusion."/>
